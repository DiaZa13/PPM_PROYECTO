<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    tools:ignore="UnusedNavigation"
    app:startDestination="@id/loginFragment">


    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.foodforme.login.loginFragment"
        android:label="FOOD FOR ME"
        tools:layout="@layout/login_fragment">
        <action
            android:id="@+id/action_loginFragment_to_newAccountFragment"
            app:destination="@id/newAccountFragment" />
        <action
            android:id="@+id/action_loginFragment_to_userPreferencesFragment"
            app:destination="@id/userPreferencesFragment" />
    </fragment>
    <fragment
        android:id="@+id/newAccountFragment"
        android:name="com.example.foodforme.newAccount.newAccountFragment"
        android:label="FOOD FOR ME"
        tools:layout="@layout/new_account_fragment" />
    <fragment
        android:id="@+id/userPreferencesFragment"
        android:name="com.example.foodforme.user_preferences.userPreferencesFragment"
        android:label="user_preferences_fragment"
        tools:layout="@layout/user_preferences_fragment" />
</navigation>